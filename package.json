{
  "name": "@devim-front/store",
  "version": "1.0.2",
  "description": "Содержит реализацию шаблона проектирования 'хранилище' и его вариации.",
  "main": "index.js",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "types": "./build/index.d.ts",
  "scripts": {
    "build:drop": "rimraf build",
    "build:make": "tsc",
    "build:docs": "run-s docs",
    "build": "run-s build:*",
    "test": "mocha -r ts-node/register ./src/index.test.ts",
    "docs:drop": "rimraf docs",
    "docs:make": "typedoc --options typedoc.json",
    "docs": "run-s docs:*",
    "release:make": "run-s build",
    "release:push": "np",
    "release:done": "npm publish",
    "release": "run-s release:*"
  },
  "keywords": [
    "devim-front",
    "store"
  ],
  "author": "gleb-mikhalkov",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.2.13",
    "@types/mocha": "^7.0.2",
    "chai": "^4.2.0",
    "husky": "^4.3.0",
    "mocha": "^7.2.0",
    "np": "^6.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.1",
    "typedoc": "^0.17.8",
    "typedoc-plugin-markdown": "^2.4.2",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devim-front/store.git"
  },
  "bugs": {
    "url": "https://github.com/devim-front/store/issues"
  },
  "homepage": "https://github.com/devim-front/store#readme",
  "dependencies": {
    "@devim-front/service": "^2.1.4"
  }
}
